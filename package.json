    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "clarifai-nodejs-grpc": "^10.1.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
