{
    "version": 2,
    "public": true,
    
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        },
        {
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "public/$1"
        }
    ]
}